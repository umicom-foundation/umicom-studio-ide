<gresources>
  <!-- All resources are served under this prefix -->
  <gresource prefix="/com/umicom/ustudio">

    <!-- Stylesheets and theme presets -->
    <file>styles/app.css</file>
    <file>styles/theme_presets.json</file>

    <!-- UI definition files -->
    <file>ui/chat.ui</file>
    <file>ui/main.ui</file>
    <file>ui/shortcuts.ui</file>

    <!-- UI partial snippets -->
    <file>ui/partials/chat_pane_snippet.xml</file>
    <file>ui/partials/log_pane_snippet.xml</file>

  </gresource>
</gresources>
